{
  "name": "git-pulse",
  "version": "1.0.0",
  "description": "CLI tool to display concise information from github repos you follow",
  "main": "index.js",
  "scripts": {
    "start": "ts-node src/index.ts",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "build": "tsc",
    "lint": "eslint --ext .ts .",
    "test": "jest --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jltwheeler/git-pulse.git"
  },
  "keywords": [
    "cli",
    "github",
    "git",
    "typescript",
    "automation"
  ],
  "author": "Josh Wheeler",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jltwheeler/git-pulse/issues"
  },
  "homepage": "https://github.com/jltwheeler/git-pulse#readme",
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.10",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "eslint": "^7.14.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "cli-table3": "^0.6.0",
    "graphql": "^15.4.0",
    "graphql-request": "^3.3.0",
    "yargs": "^16.1.1"
  }
}
